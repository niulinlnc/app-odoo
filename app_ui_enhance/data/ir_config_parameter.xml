<?xml version="1.0"?>
<openerp>
    <data noupdate="0">
        <record id="app_ui_show_search_date" model="ir.config_parameter">
            <field name="key">app_ui_show_search_date</field>
            <field name="value">True</field>
        </record>
        <record id="app_ui_show_search_number" model="ir.config_parameter">
            <field name="key">app_ui_show_search_number</field>
            <field name="value">False</field>
        </record>
        <record id="app_ui_force_barcode" model="ir.config_parameter">
            <field name="key">app_ui_force_barcode</field>
            <field name="value">Code128</field>
        </record>
        <!--默认允许QR，Code39-->
        <record id="app_ui_allow_barcode" model="ir.config_parameter">
            <field name="key">app_ui_allow_barcode</field>
            <field name="value">QR,Standard39</field>
        </record>
    </data>
</openerp>